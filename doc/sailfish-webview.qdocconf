project         = Sailfish WebView
description     = Sailfish Webview Reference Documentation
versionsym      =
version         = 1.0.0
url             = https://github.com/sailfishos/sailfish-webview/

sourcedirs += $$PWD/../lib $$PWD/../import $$PWD/../doc
headerdirs += $$PWD/../lib $$PWD/../import $$PWD/../doc
exampledirs += $$PWD/../examples

outputformats = HTML
outputdir = $$PWD/../doc/html
base = file:$$PWD/../doc/html
tagfile = $$PWD/../doc/html/sailfishos-webview.tags

outputprefixes = QML
outputprefixes.QML = qml-sailfishos-webview-

qhp.projects = SailfishWebView

qhp.SailfishWebView.file = sailfish-webview.qhp
qhp.SailfishWebView.namespace = org.sailfishos.wevview.1.0.0
qhp.SailfishWebView.virtualFolder = sailfishos-webview
qhp.SailfishWebView.indexTitle = Sailfish WebView
qhp.SailfishWebVoew.indexRoot =

qhp.SailfishWebView.filterAttributes = sailfishos-webview 1.0.0
qhp.SailfishWebView.customFilters.SailfishWebView.name = Sailfish WebView 1.0.0
qhp.SailfishWebView.customFilters.SailfishWebView.filterAttributes = sailfishos-webview 1.0.0

qhp.SailfishWebView.subprojects        = overview qmltypes classes

qhp.SailfishWebView.subprojects.overview.title = Sailfish WebView Overview
qhp.SailfishWebView.subprojects.overview.indexTitle = Sailfish WebView Overview
qhp.SailfishWebView.subprojects.overview.type = manual
qhp.SailfishWebView.subprojects.qmltypes.title = QML Types
qhp.SailfishWebView.subprojects.qmltypes.indexTitle = Sailfish.WebView QML Types
qhp.SailfishWebView.subprojects.qmltypes.selectors = qmlclass
qhp.SailfishWebView.subprojects.qmltypes.sortPages = true
qhp.SailfishWebView.subprojects.classes.title = C++ Classes
qhp.SailfishWebView.subprojects.classes.indexTitle = Sailfish WebView C++ Classes
qhp.SailfishWebView.subprojects.classes.selectors = class fake:headerfile
qhp.SailfishWebView.subprojects.classes.sortPages = true

